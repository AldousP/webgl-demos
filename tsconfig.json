{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "sourceMap": true,
    "alwaysStrict": true,
    "lib": ["es2015", "dom", "dom.iterable"],
    "downlevelIteration": true,
    "esModuleInterop": true,
    "baseUrl": ".",
    "paths": {
      "@app/*": [ "src/*" ],
      "static/*": [ "static/*" ]
    },
    "jsx": "react",
    "allowJs": true,
    "allowSyntheticDefaultImports": true
  },
  "types": [ "node" ]
}